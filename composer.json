{
    "name": "yiisoft/yii2-elasticsearch",
    "description": "Elasticsearch integration and ActiveRecord for the Yii framework",
    "keywords": [
        "yii2",
        "elasticsearch",
        "active-record",
        "search",
        "fulltext"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2-elasticsearch/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "source": "https://github.com/yiisoft/yii2-elasticsearch"
    },
    "authors": [
        {
            "name": "Carsten Brandt",
            "email": "mail@cebe.cc"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "yiisoft/yii2": "2.2.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": { "yii\\elasticsearch\\": "src" }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "22.0.x-dev"
        }
    }
}
